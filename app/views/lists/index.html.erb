<h1>To Do Lists</h1>

<%= render partial: "form" %>

<ul>
  <% @lists.each do |list| %>
    <li>
      <%= link_to list.name, list_path(list)%>
      <%= button_to "edit", edit_list_path(list) %>
      <%= button_to 'delete', list, method: :delete, data: {confirm: 'Are you sure?'} %>
    </li> 
  <% end %>
</ul>
